css","name":null,"original":{"line":267,"column":8},"generated":{"line":228,"column":2}}],"sources":{"style.scss":"* {\n  margin: 0;\n  padding: 0;\n  border: 0;\n  box-sizing: border-box;\n}\n\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@400;700&display=swap\");\n\n// colors\n\n$primary: #383cc1;\n$success: #22be70;\n$danger: #bf3325;\n$dark: #121614;\n$light: #a4b1aa;\n\n$boxShadow: 3px 5px 8px 2px;\n\n$transition: all 250ms ease-out;\n\nbody {\n  font-family: \"Poppins\", sans-serif;\n  display: grid;\n  color: aliceblue;\n  background: #1c1c1c;\n  margin: 10px;\n}\n\n.hidden {\n  visibility: hidden;\n  height: 0;\n}\n\n.main-container {\n  height: 100vh;\n}\n\n.form-container {\n  max-width: 768px;\n  margin: 1rem auto;\n}\n\nh1 {\n  font-size: 3rem;\n  text-align: center;\n}\n\nh3 {\n  text-align: center;\n  color: $success;\n  margin: 1rem;\n}\n\n.message {\n  margin: 1rem 0 0.5rem;\n  color: white;\n  padding: 1rem;\n  text-align: center;\n\n  &.success-message {\n    background-color: $success;\n    box-shadow: 2px 4px 7px 2px rgba($success, 0.2);\n  }\n  &.remove-message {\n    background-color: $primary;\n    box-shadow: 2px 4px 7px 2px rgba($primary, 0.2);\n  }\n  &.alert-message {\n    background-color: $danger;\n    box-shadow: 2px 4px 7px 2px rgba($danger, 0.2);\n  }\n}\n\nform {\n  display: grid;\n  grid-gap: 1rem;\n  margin: 1rem 0;\n  width: 100%;\n\n  label,\n  input,\n  text-area {\n    font-family: inherit;\n    font-size: inherit;\n  }\n\n  [for=\"title\"],\n  [for=\"note\"] {\n    display: flex;\n    align-items: center;\n    flex-wrap: wrap;\n    padding: 1rem;\n    grid-gap: 1rem;\n    // background-color: rgba($light, 0.2);\n    font-weight: bold;\n  }\n\n  [for=\"title\"] {\n    input {\n      flex: 1 1 80%;\n      //   border: 3px solid $light;\n      padding: 0.5rem;\n      color: $dark;\n      width: 100%;\n\n      &:focus {\n        outline: 2px solid $dark;\n      }\n    }\n  }\n\n  input,\n  textarea {\n    flex: 1 1 80%;\n    // border: 3px solid $light;\n    border-radius: 10px;\n    padding: 0.5rem;\n    color: $dark;\n    width: 100%;\n    background-color: rgb(217, 223, 235);\n\n    &:focus {\n      outline: 2px solid $dark;\n    }\n  }\n  [type=\"submit\"] {\n    background-color: $success;\n    box-shadow: $boxShadow rgba($success, 0.2);\n    border: 3px solid transparent;\n    border-radius: 10px;\n    font-size: 1.3rem;\n    color: white;\n    padding: 0.5rem;\n    cursor: pointer;\n    transition: all 250ms ease-in-out;\n\n    &:hover,\n    &:focus {\n      outline: none;\n      box-shadow: none;\n      border: 3px solid rgba($dark, 0.5);\n    }\n  }\n}\n\n.scroll-down {\n  text-align: center;\n  margin: 100px;\n}\n\n.note-container {\n  margin: 2rem auto 1rem;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n  grid-gap: 1.5rem;\n\n  .note {\n    border: 3px solid rgba(aliceblue, 0.2);\n    border-radius: 10px;\n    padding: 1.5rem;\n    transition: $transition;\n  }\n\n  .note-title {\n    font-size: 2rem;\n    text-align: center;\n    color: #1c8d73;\n    margin-bottom: 0.5rem;\n  }\n\n  .note-body {\n    overflow: hidden;\n    max-height: 8rem;\n    -webkit-box-orient: vertical;\n    display: -webkit-box;\n    text-overflow: ellipsis;\n    -webkit-line-clamp: 4;\n  }\n\n  .note-btns {\n    display: grid;\n    grid-template-columns: auto auto;\n    grid-gap: 0.5rem;\n\n    .note-btn {\n      font-family: inherit;\n      font-size: inherit;\n      padding: 0.5rem 1rem;\n      margin-top: 1rem;\n      color: white;\n      cursor: pointer;\n      transition: $transition;\n      border-radius: 5px;\n\n      &:focus,\n      &:hover {\n        box-shadow: none;\n      }\n    }\n\n    .note-view {\n      background-color: $primary;\n      box-shadow: $boxShadow rgba($primary, 0.2);\n    }\n\n    .note-delete {\n      background-color: $danger;\n      box-shadow: $boxShadow rgba($danger, 0.2);\n    }\n  }\n}\n\n.modal-container {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba($dark, 0.5);\n  display: grid;\n  place-items: center;\n  opacity: 0;\n  pointer-events: none;\n  transition: $transition;\n\n  &.active {\n    opacity: 1;\n    pointer-events: all;\n  }\n\n  .modal {\n    max-width: 700px;\n    max-height: 80vh;\n    overflow: auto;\n    background-color: aliceblue;\n    padding: 2rem;\n    border: 3px solid $dark;\n    color: #121614;\n    border-radius: 20px;\n    position: relative;\n\n    .modal-title {\n      text-align: center;\n      margin-bottom: 1rem;\n      font-size: 2rem;\n      line-height: 1.2rem;\n      //   max-width: 80%;\n    }\n\n    .modal-btn {\n      position: absolute;\n      top: 0.7rem;\n      right: 0.7rem;\n      font-size: 2rem;\n      height: 40px;\n      width: 40px;\n      border: 3px solid $dark;\n      border-radius: 50%;\n      background-color: aliceblue;\n      box-shadow: $boxShadow rgba($dark, 0.5);\n      transition: $transition;\n\n      &:hover,\n      &:focus {\n        outline: none;\n        box-shadow: none;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"cea4be158545012e8d52c0f55a693cfc","cacheData":{"env":{}}}